<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.0" />
    </system.web>

    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="MetodosRCDPortBinding" />
                <binding name="MetodosRCDPortBinding1" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8081/WSServerSOAP/MetodosRCD"
                binding="basicHttpBinding" bindingConfiguration="MetodosRCDPortBinding"
                contract="WSServerSoap.MetodosRCD" name="MetodosRCDPort" />
            <endpoint address="http://localhost:8081/WSServerSOAP/MetodosRCD"
                binding="basicHttpBinding" bindingConfiguration="MetodosRCDPortBinding1"
                contract="ServiceReference1.MetodosRCD" name="MetodosRCDPort1" />
        </client>
    </system.serviceModel>
</configuration>
